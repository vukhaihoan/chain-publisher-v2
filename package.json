{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "postinstall": "yarn && yarn build"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "chain-publisher",
  "author": "ThienCao",
  "module": "dist/chain-publisher.esm.js",
  "size-limit": [
    {
      "path": "dist/chain-publisher.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/chain-publisher.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@ethersproject/providers": "https://github.com/ezDeFi/ethers.js/releases/download/v5.3.2.1/ethers_providers.tar.gz",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/bluebird": "^3.5.36",
    "@types/jest": "^28.1.4",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.1",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "mocha": "^10.0.0",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "ethers": "^5.6.9",
    "lodash": "^4.17.21"
  }
}
